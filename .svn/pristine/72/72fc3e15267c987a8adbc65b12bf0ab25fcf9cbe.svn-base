@charset "ISO-8859-1";

#PersonDataInformerAdmin .oep-table tr:nth-child(2n+1) td {
	background: #f7f7f7 !important;
}

#PersonDataInformerAdmin .oep-table td,
#PersonDataInformerAdmin .oep-table th {
	background: #ffffff;
	text-align: left;
	padding: 13px 10px;
	border-right: 1px solid #fff;
}

#PersonDataInformerAdmin .oep-table tr td {
	background: #fff !important;
}

#PersonDataInformerAdmin .oep-table tbody tr td {
	border-bottom: 1px solid #fff;
}

#PersonDataInformerAdmin table.oep-table thead tr {
	border-top: 2px solid #424242;
}

#PersonDataInformerAdmin .oep-table td.icon,
#PersonDataInformerAdmin .oep-table th.icon {
	text-align: center;
	width: 15px;
}
#PersonDataInformerAdmin .oep-table td.link,
#PersonDataInformerAdmin .oep-table th.link {
	text-align: right;
	white-space: nowrap;
}
	#PersonDataInformerAdmin .oep-table td.link .btn,
	#PersonDataInformerAdmin .oep-table th.link .btn {
		display: inline-block;
	}
		@media screen and (min-width: 766.99px) {
			#PersonDataInformerAdmin td.link a.btn:not(:last-child) {
				margin-right: 3px;
			}
		}